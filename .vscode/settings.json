{
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.linting.flake8Args": ["--max-line-length=150"],
  "flake8.interpreter": ["venv/bin/python3"],
  "python.formatting.provider": "none",
  "cSpell.words": [
    "astype",
    "dataframe",
    "figsize",
    "imshow",
    "kmeans",
    "lemmatize",
    "Lemmatizer",
    "matplotlib",
    "Multinomial",
    "ndarray",
    "nltk",
    "openai",
    "pyplot",
    "savefig",
    "scatterplot",
    "seaborn",
    "stopwords",
    "Tfidf",
    "toarray",
    "TSNE",
    "vectorizer",
    "wordcloud",
    "wordnet"
  ]
}
